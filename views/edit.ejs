<a href='/' class='button is-danger is-small' style='margin-bottom: 2em'>
    Back
</a>
<form action=<%= '/update/' + result._id + '?_method=PUT' %> method='POST'>
    <div class='field'>
        <div class='control'>
            <input class='input' type='text' placeholder='name' name='name' value='<%= result.name %>'>
        </div>
    </div>
    <div class='field'>
        <div class='control'>
            <input class='input' type='text' placeholder='result' name='result' value='<%= result.result %>'>
        </div>
    </div>
    <button type='submit' class='button is-success is-small'>Submit</button>
</form>